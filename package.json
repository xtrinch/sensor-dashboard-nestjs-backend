{
  "name": "sensor-dashboard-nestjs-backend",
  "version": "0.0.1",
  "description": "",
  "author": "xtrinch",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "yarn run prettier && yarn run lint",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:tsnode": "env-cmd -f .env ts-node --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:dev": "env-cmd -f .env ts-node-dev --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f .env.test jest --runInBand --verbose",
    "test:watch": "env-cmd -f .env.test jest --watch",
    "test:cov": "env-cmd -f .env.test jest --coverage",
    "test:debug": "env-cmd -f .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f .env.test jest --runInBand --config ./jest-e2e.json --testTimeout 99999999 --verbose",
    "typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.5",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.8",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/draft-js": "^0.10.44",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/tar": "^4.0.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.1.0",
    "google-auth-library": "^6.1.3",
    "koofr": "^0.2.0",
    "lodash": "^4.17.21",
    "mqtt": "^4.2.6",
    "nestjs-typeorm-paginate": "^2.2.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "tar": "^6.1.2",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.4",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
